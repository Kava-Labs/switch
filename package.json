    "interledger",
    "wallet",
    "lightning",
    "btc",
    "ethereum",
    "xrp",
    "trade",
    "exchange",
    "streaming",
    "micropayments"
  ],
  "author": "Kava Labs <hello@kava.io> (https://kava.io/)",
  "contributors": [
    "Kincaid O'Neil <kincaid@kava.io> (https://kincaidoneil.com/)"
  ],
  "license": "Apache-2.0",
